    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS", // 디버깅 이름
        "runtimeExecutable": "npm",  // 실행 인터프리터
        "runtimeArgs": [
          // npm run start:debug
          "run",
          "start:debug",
        ],
        "console": "integratedTerminal", // 터미널로 셋팅
        "restart": true, 
        "port": 9229,
        "autoAttachChildProcesses": true
      }
    ]
  }